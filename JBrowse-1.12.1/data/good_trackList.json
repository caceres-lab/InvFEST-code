{
   "formatVersion" : 1,
   "tracks" : [
      {
         "urlTemplate" : "seq/{refseq_dirpath}/{refseq}-",
         "metadata" : {
            "general_tracks" : "Reference Sequence"
         },
         "type" : "SequenceTrack",
         "chunkSize" : 20000,
         "category" : "Reference sequence",
         "key" : "Reference sequence",
         "storeClass" : "JBrowse/Store/Sequence/StaticChunked",
         "label" : "DNA"
      },

      {
         "fmtDetailValue_Name" : "function(name) { return name+'&nbsp;&nbsp;&nbsp;&nbsp;<a href=\"https://www.ncbi.nlm.nih.gov/gquery/?term='+name+'\" title=\"Search at NCBI\" target=\"_blank\">[NCBI]</a>&nbsp;&nbsp;<a href=\"https://genome.ucsc.edu/cgi-bin/hgGene?hgg_gene='+name+'&db=hg19\" title=\"Search at UCSC\" target=\"_blank\">[UCSC]</a>'; }",
         "transcriptType" : [
            "mRNA",
            "transcript"
         ],
         "style" : {
            "className" : "feature",
            "color" : "function(feature) { if (feature.get('type')=='exon') {return '#12738A';} else return '#daa520';}",
            "borderWidth" : 0,
            "text2Color" : "#595959",
            "height" : "function(feature) { if (feature.get('type')=='exon') {return 6;} else return 16;}"
         },
         "fmtDetailValue_Integrativemkt" : "function(integrativemkt) { return '<a href=\"//pophuman.uab.cat/cgi-bin/R/iMKT?geneid='+integrativemkt+'\" target=\"_blank\">See results in a new window</a>'; }",
         "subParts" : [
            "exon",
            "CDS",
            "five_prime_UTR",
            "three_prime_UTR"
         ],
         "displayMode": "compact",
         "menuTemplate" : [
            {
               "iconClass" : "dijitIconTask",
               "action" : "contentDialog",
               "title" : "{type} {name}",
               "label" : "View details"
            },
            {
               "iconClass" : "dijitIconFilter"
            },
            {
               "iconClass" : "dijitIconDatabase",
               "url" : "https://www.ncbi.nlm.nih.gov/gquery/?term={name}",
               //"action" : "iframeDialog",
               "action" : "newWindow",
               //"title" : "Searching for {name} at NCBI",
               "label" : "Search for {name} at NCBI"
            },
            {
               "iconClass" : "dijitIconDatabase",
               "url" : "https://genome.ucsc.edu/cgi-bin/hgGene?hgg_gene={id}&db=hg19",
               //"action" : "iframeDialog",
               "action" : "newWindow",
               //"title" : "Searching for {name} at UCSC genes",
               "label" : "Search for {name} at UCSC genes"
            }//,
            // {
            //    "iconClass" : "dijitIconTable",
