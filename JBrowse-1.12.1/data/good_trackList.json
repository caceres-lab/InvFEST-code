            //    "url" : "https://pophuman.uab.cat/cgi-bin/R/iMKT?geneid={integrativemkt}",
            //    "label" : "Integrative MKT for {name}",
            //    "title" : "Integrative MKT for {name}"
            // }
         ],
         "key" : "Gene annotations",
         "category" : "Descriptive tracks",
         "impliedUTRs" : true,
         "trackType" : "CanvasFeatures",
         "storeClass" : "JBrowse/Store/SeqFeature/NCList",
         "urlTemplate" : "tracks/gene_annotations/{refseq}/trackData.json", 
         "histograms" : {
            "color" : "#85b2cb"
         },
         "compress" : 0,
         "label" : "gene_annotations",
         "type" : "CanvasFeatures",
         "metadata" : {
            "mapping_and_sequencing" : "Gene annotations",
